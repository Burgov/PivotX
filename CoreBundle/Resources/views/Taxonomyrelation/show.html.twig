{% extends 'PivotXCoreBundle::base.html.twig' %}

{% block crud %}

<h1>Taxonomyrelation</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Sortingorder</th>
            <td>{{ entity.sortingOrder }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('taxonomyrelation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('taxonomyrelation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('taxonomyrelation_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="btn btn-danger">Delete</button>
        </form>
    </li>
</ul>

{% endblock %}
